{
	"project": {
		"name": "Sistema de Monitoreo de Memoria y Automatización ChatGPT",
		"version": "2.0.0",
		"description": "Sistema completo para monitoreo de memoria, captura de GUI y automatización de ChatGPT web",
		"author": "Desarrollador",
		"license": "MIT"
	},
	"compilation": {
		"compiler": "g++",
		"optimization": "-O2",
		"warnings": "-Wall -Wextra",
		"standard": "-std=c++17",
		"timeout": 30,
		"modules": [
			{
				"name": "memory_monitor",
				"source": "memory_monitor.cpp",
				"output": "memory_monitor.exe",
				"libraries": "-lgdi32 -luser32 -lpsapi"
			},
			{
				"name": "gui_capture",
				"source": "gui_capture.cpp",
				"output": "gui_capture.exe",
				"libraries": "-lgdi32 -luser32 -lpsapi"
			},
			{
				"name": "chatgpt_automation",
				"source": "chatgpt_automation.cpp",
				"output": "chatgpt_automation.exe",
				"libraries": "-lgdi32 -luser32 -lole32 -loleaut32 -lpsapi"
			}
		]
	},
	"security": {
		"enabled": true,
		"require_admin": false,
		"allowed_processes": [
			"chrome.exe",
			"msedge.exe",
			"firefox.exe",
			"opera.exe",
			"brave.exe",
			"safari.exe",
			"iexplore.exe"
		],
		"allowed_domains": [
			"chat.openai.com",
			"openai.com"
		]
	},
	"chatgpt": {
		"web_automation": {
			"enabled": true,
			"url": "https://chat.openai.com",
			"timeout": 30,
			"retry_attempts": 3
		},
		"demo": {
			"enabled": true,
			"languages": [
				"python",
				"javascript",
				"powershell",
				"cpp",
				"java",
				"csharp"
			]
		}
	},
	"monitoring": {
		"memory": {
			"enabled": true,
			"interval": 5000,
			"max_processes": 100
		},
		"gui": {
			"enabled": true,
			"format": "bmp",
			"quality": "high",
			"max_captures": 10
		}
	},
	"vscode": {
		"optimization": {
			"enabled": true,
			"max_memory_mb": 16384,
			"max_editors": 8,
			"timeouts": {
				"build": 30,
				"debug": 60,
				"task": 120
			}
		},
		"exclusions": [
			"**/*.exe",
			"**/*.bmp",
			"**/*.obj",
			"**/*.o",
			"**/generated_code.*",
			"**/capture_*.bmp"
		]
	},
	"performance": {
		"optimizations": {
			"compilation": "-O2",
			"memory_management": "efficient",
			"file_watching": "selective",
			"search": "optimized"
		},
		"limits": {
			"max_file_size_mb": 16,
			"max_search_results": 1000,
			"terminal_scrollback": 1000
		}
	},
	"features": {
		"memory_monitoring": true,
		"gui_capture": true,
		"chatgpt_automation": true,
		"security_whitelist": true,
		"vscode_integration": true,
		"performance_optimization": true
	},
	"maintenance": {
		"cleanup": {
			"auto_clean": false,
			"backup_before_clean": true,
			"patterns": [
				"*.exe",
				"*.obj",
				"*.o",
				"generated_code.*",
				"capture_*.bmp",
				"*.log",
				"*.tmp"
			]
		},
		"backup": {
			"enabled": true,
			"location": "./backup",
			"retention_days": 7
		}
	},
	"documentation": {
		"readme": "README.md",
		"api_docs": false,
		"examples": true,
		"troubleshooting": true
	},
	"development": {
		"debug": {
			"enabled": true,
			"symbols": true,
			"logging": "verbose"
		},
		"testing": {
			"enabled": false,
			"framework": "none",
			"coverage": false
		}
	}
}
