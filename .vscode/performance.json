{
	"performance": {
		"memory": {
			"maxMemoryForLargeFilesMB": 16384,
			"maxMemoryForLargeFilesMB": 32768,
			"maxMemoryForLargeFilesMB": 65536
		},
		"files": {
			"watcherExclude": {
				"**/node_modules/**": true,
				"**/dist/**": true,
				"**/build/**": true,
				"**/.git/**": true,
				"**/generated_code.*": true,
				"**/*.bmp": true,
				"**/*.exe": true,
				"**/*.obj": true,
				"**/*.o": true,
				"**/*.a": true,
				"**/*.lib": true,
				"**/*.dll": true,
				"**/*.so": true,
				"**/*.dylib": true
			},
			"exclude": {
				"**/node_modules": true,
				"**/dist": true,
				"**/build": true,
				"**/.git": true,
				"**/*.bmp": true,
				"**/*.exe": true,
				"**/*.obj": true,
				"**/*.o": true,
				"**/*.a": true,
				"**/*.lib": true,
				"**/*.dll": true,
				"**/*.so": true,
				"**/*.dylib": true
			}
		},
		"search": {
			"exclude": {
				"**/node_modules": true,
				"**/dist": true,
				"**/build": true,
				"**/.git": true,
				"**/generated_code.*": true,
				"**/*.bmp": true,
				"**/*.exe": true,
				"**/*.obj": true,
				"**/*.o": true,
				"**/*.a": true,
				"**/*.lib": true,
				"**/*.dll": true,
				"**/*.so": true,
				"**/*.dylib": true
			},
			"maxResults": 1000,
			"followSymlinks": false,
			"useGlobalIgnoreFiles": true,
			"useParentIgnoreFiles": true,
			"useIgnoreFiles": true
		},
		"editor": {
			"minimap": {
				"enabled": false
			},
			"wordWrap": "off",
			"renderWhitespace": "none",
			"renderControlCharacters": false,
			"renderLineHighlight": "line",
			"cursorBlinking": "solid",
			"cursorSmoothCaretAnimation": "off",
			"smoothScrolling": false,
			"mouseWheelScrollSensitivity": 1,
			"fontSize": 12,
			"fontFamily": "Consolas, 'Courier New', monospace",
			"fontLigatures": false,
			"tabSize": 4,
			"insertSpaces": true,
			"detectIndentation": false
		},
		"workbench": {
			"editor": {
				"enablePreview": false,
				"enablePreviewFromQuickOpen": false,
				"closeOnFileDelete": true,
				"closeEmptyGroups": true,
				"limit": {
					"enabled": true,
					"value": 8,
					"perEditorGroup": true
				}
			},
			"startupEditor": "none",
			"showTabs": "singleClick",
			"decorations": {
				"badges": false,
				"colors": false
			},
			"activityBar": {
				"visible": true
			},
			"sideBar": {
				"location": "left"
			},
			"panel": {
				"defaultLocation": "bottom"
			},
			"statusBar": {
				"visible": true
			}
		},
		"terminal": {
			"integrated": {
				"scrollback": 1000,
				"maximumLinks": 100,
				"gpuAcceleration": "on",
				"rendererType": "canvas",
				"shell": {
					                "windows": "powershell.exe"
				},
				"defaultProfile": {
					"windows": "PowerShell"
				},
				"profiles": {
					"windows": {
						"PowerShell": {
							"source": "PowerShell",
							"icon": "terminal-powershell"
						}
					}
				}
			}
		},
		"extensions": {
			"autoUpdate": false,
			"autoCheckUpdates": false,
			"ignoreRecommendations": true
		},
		"git": {
			"enabled": true,
			"autorefresh": false,
			"autofetch": false,
			"confirmSync": false,
			"enableSmartCommit": true,
			"enableCommitSigning": false
		},
		"debug": {
			"console": {
				"closeOnEnd": true
			},
			"inlineValues": false,
			"showBreakpointsInOverviewRuler": false,
			"showInlineBreakpointCandidates": false
		},
		"task": {
			"autoDetect": "off",
			"slowProviderWarning": false
		},
		"problems": {
			"decorations": {
				"enabled": false
			},
			"showCurrentInStatus": false
		},
		"explorer": {
			"confirmDelete": false,
			"confirmDragAndDrop": false,
			"incrementalNaming": "smart",
			"compactFolders": false
		},
		"notifications": {
			"enable": false
		},
		"update": {
			"mode": "none",
			"showReleaseNotes": false
		},
		"telemetry": {
			"telemetryLevel": "off",
			"enableCrashReporter": false,
			"enableTelemetry": false
		},
		"sync": {
			"enabled": false,
			"autoDownload": false,
			"autoUpload": false
		},
		"files": {
			"backup": false,
			"hotExit": "off",
			"autoSave": "afterDelay",
			"autoSaveDelay": 1000,
			"trimTrailingWhitespace": true,
			"insertFinalNewline": true,
			"trimFinalNewlines": true,
			"eol": "\n",
			"encoding": "utf8"
		}
	}
}
